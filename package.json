{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p 3000",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "export": "yarn run build && next export",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.2",
    "babel-plugin-styled-components": "^1.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "firebase": "^7.19.1",
    "gray-matter": "^4.0.2",
    "next": "latest",
    "next-pwa": "3.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "remark": "^12.0.1",
    "remark-html": "^12.0.0",
    "styled-components": "^5.1.1",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "typescript": "4.0"
  },
  "license": "MIT",
  "browser": {
    "child_process": false
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
