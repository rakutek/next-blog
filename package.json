{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p 3000",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "deploy": "yarn run build && next export && firebase deploy"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.2",
    "babel-plugin-styled-components": "^1.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "firebase": "^7.19.1",
    "github-markdown-css": "^4.0.0",
    "gray-matter": "^4.0.2",
    "highlight.js": "^10.4.1",
    "next": "latest",
    "next-pwa": "3.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.13.1",
    "react-markdown": "^4.3.1",
    "rehype-highlight": "^4.0.0",
    "remark": "^12.0.1",
    "remark-breaks": "^2.0.0",
    "remark-emoji": "^2.1.0",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^12.0.0",
    "remark-parse": "^8.0.3",
    "remark-react-lowlight": "^0.7.0",
    "remark-rehype": "^7.0.0",
    "styled-components": "^5.1.1",
    "styled-media-query": "^2.1.2",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "react-syntax-highlighter": "^13.5.1",
    "rehype-katex": "^4.0.0",
    "rehype-react": "^6.1.0",
    "remark-math": "^3.0.1",
    "remark-react": "^7.0.1",
    "to-vfile": "^6.1.0",
    "typescript": "4.0",
    "unified": "^9.2.0",
    "vfile-reporter": "^6.0.1"
  },
  "license": "MIT",
  "browser": {
    "child_process": false
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
